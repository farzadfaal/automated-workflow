{
  "name": "blog",
  "version": "1.0.0",
  "description": "",
  "main": "gulpfile.js",
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^9.8.0",
    "browser-sync": "^2.26.7",
    "cssnano": "^4.1.10",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.2",
    "gulp-cached": "^1.1.1",
    "gulp-concat": "^2.6.1",
    "gulp-filter": "^6.0.0",
    "gulp-iconfont": "^10.0.3",
    "gulp-iconfont-css": "^3.0.0",
    "gulp-image": "^6.1.0",
    "gulp-load-plugins": "^2.0.3",
    "gulp-newer": "^1.4.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-print": "^5.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-rtlcss": "^1.4.1",
    "gulp-sass": "^4.1.0",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "node-sass": "^4.14.1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "paths": {
    "src": {
      "base": "./src",
      "sass": "./src/scss/",
      "img": "./src/img/",
      "js": "./src/js/",
      "fonts": "./src/fonts/",
      "icons": "./src/icons/"
    },
    "build": {
      "base": "./build/",
      "css": "./build/css/",
      "js": "./build/js/"
    },
    "dist": {
      "base": "./dist/",
      "css": "./dist/css/",
      "img": "./dist/img/",
      "js": "./dist/js/",
      "fonts": "./dist/fonts/"
    },
    "sass": [
      "node_modules"
    ]
  },
  "globs": {
    "distCss": [
      "./build/css/*.css"
    ],
    "distJs": [
      "./build/js/*.js"
    ]
  },
  "vars": {
    "sassName": "style.scss",
    "cssName": "style.css",
    "siteCssName": "style.min.css",
    "siteRtlCssName": "style.min.rtl.css",
    "siteIconFontSassName": "_iconfont.scss"
  }
}
