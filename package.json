  "paths": {
    "src": {
      "base": "./src",
      "sass": "./src/scss/",
      "img": "./src/img/",
      "js": "./src/js/",
      "fonts": "./src/fonts/",
      "icons": "./src/icons/"
    },
    "build": {
      "base": "./build/",
      "css": "./build/css/",
      "js": "./build/js/"
    },
    "dist": {
      "base": "./dist/",
      "css": "./dist/css/",
      "img": "./dist/img/",
      "js": "./dist/js/",
      "fonts": "./dist/fonts/"
    },
    "sass": [
      "node_modules"
    ]
  },
  "globs": {
    "distCss": [
      "./build/css/*.css"
    ],
    "distJs": [
      "./build/js/*.js"
    ]
  },
  "vars": {
    "sassName": "style.scss",
    "cssName": "style.css",
    "siteCssName": "style.min.css",
    "siteRtlCssName": "style.min.rtl.css",
    "siteIconFontSassName": "_iconfont.scss",
    "siteJsName": "main.js",
    "siteBundledJsName": "main.min.js"
  }
